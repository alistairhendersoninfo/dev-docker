          "manual": {
            "script": "curl -fsSL https://dev.mysql.com/get/Downloads/MySQL-{version}/mysql-{version}-linux-glibc2.12-x86_64.tar.gz | tar -xzf -",
            "env_vars": {
              "PATH": "/usr/local/mysql{version}/bin:$PATH"
            }
          }
        },
        "alpine": {
          "apk": {
            "packages": ["mysql{version}", "mysql-client"]
