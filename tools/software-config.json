{
  "development_stacks": {
    "nodejs": {
      "name": "Node.js",
      "description": "JavaScript/TypeScript runtime and package manager",
      "install_methods": {
        "ubuntu": {
          "apt": {
            "packages": ["nodejs", "npm"],
            "repository": "deb https://deb.nodesource.com/node_20.x $(lsb_release -cs) main",
            "key": "https://deb.nodesource.com/gpgkey/nodesource-repo.gpg.key"
          },
          "manual": {
            "script": "curl -fsSL https://deb.nodesource.com/setup_20.x | sudo -E bash -",
            "packages": ["nodejs"]
          }
        },
        "alpine": {
          "apk": {
            "packages": ["nodejs", "npm"]
          }
        }
      },
      "versions": {
        "latest": "21.x",
        "lts": "20.x",
        "current": "21.x",
        "21": "21.0.0",
        "20": "20.10.0",
        "19": "19.9.0",
        "18": "18.19.0",
        "17": "17.9.1",
        "16": "16.20.2",
        "15": "15.14.0",
        "14": "14.21.3",
        "13": "13.14.0",
        "12": "12.22.12",
        "11": "11.15.0",
        "10": "10.24.1",
        "9": "9.11.2",
        "8": "8.17.0",
        "7": "7.10.1",
        "6": "6.17.1"
      },
      "post_install": [
        "npm install -g npm@latest",
        "npm install -g yarn"
      ]
    },
    "python": {
      "name": "Python",
      "description": "Python interpreter and pip package manager",
      "install_methods": {
        "ubuntu": {
          "apt": {
            "packages": ["python3", "python3-pip", "python3-venv", "python3-dev", "build-essential"]
          },
          "manual": {
            "script": "curl -fsSL https://www.python.org/ftp/python/{version}/Python-{version}.tgz | tar -xzf -",
            "env_vars": {
              "PATH": "/usr/local/python{version}/bin:$PATH"
            }
          }
        },
        "alpine": {
          "apk": {
            "packages": ["python3", "py3-pip", "python3-dev", "build-base"]
          }
        }
      },
      "versions": {
        "latest": "3.12",
        "stable": "3.11",
        "legacy": "3.10",
        "3.9": "3.9.18",
        "3.8": "3.8.18",
        "3.7": "3.7.17",
        "3.6": "3.6.15",
        "3.5": "3.5.10",
        "3.4": "3.4.10",
        "3.3": "3.3.7",
        "3.2": "3.2.6",
        "3.1": "3.1.5",
        "3.0": "3.0.1"
      },
      "post_install": [
        "pip3 install --upgrade pip",
        "pip3 install virtualenv"
      ]
    },
    "rust": {
      "name": "Rust",
      "description": "Systems programming language and Cargo",
      "install_methods": {
        "universal": {
          "script": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y",
          "post_script": "source $HOME/.cargo/env"
        }
      },
      "versions": {
        "latest": "stable",
        "stable": "stable",
        "nightly": "nightly"
      },
      "post_install": [
        "cargo install cargo-edit",
        "cargo install cargo-watch"
      ]
    },
