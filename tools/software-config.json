  "databases": {
    "postgresql": {
      "name": "PostgreSQL",
      "description": "Advanced open source database",
      "install_methods": {
        "ubuntu": {
          "apt": {
            "packages": ["postgresql-{version}", "postgresql-{version}-contrib", "postgresql-client"],
            "repository": "deb http://apt.postgresql.org/pub/repos/apt $(lsb_release -cs)-pgdg main",
            "key": "https://www.postgresql.org/media/keys/ACCC4CF8.asc"
          },
          "manual": {
            "script": "curl -fsSL https://www.postgresql.org/ftp/source/v{version}/postgresql-{version}.tar.gz | tar -xzf -",
            "env_vars": {
              "PATH": "/usr/local/pgsql{version}/bin:$PATH"
            }
          }
        },
        "alpine": {
          "apk": {
            "packages": ["postgresql{version}", "postgresql{version}-contrib", "postgresql-client"]
          }
        }
      },
      "versions": {
        "latest": "16",
        "stable": "15",
        "lts": "14",
        "16": "16.0",
        "15": "15.4",
        "14": "14.11",
        "13": "13.14",
        "12": "12.18",
        "11": "11.23",
        "10": "10.23"
      },
      "default_port": 5432,
      "config_files": ["/etc/postgresql/*/main/postgresql.conf", "/etc/postgresql/*/main/pg_hba.conf"]
    },
    "mysql": {
      "name": "MySQL",
