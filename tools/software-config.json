          }
        },
        "alpine": {
          "apk": {
            "packages": ["grafana"]
          }
        }
      },
      "versions": {
        "latest": "10.2",
        "stable": "10.2",
        "beta": "10.3"
      },
      "default_port": 3000,
      "config_files": ["/etc/grafana/grafana.ini"]
    },
    "elk": {
      "name": "ELK Stack",
      "description": "Log management and analysis",
      "install_methods": {
        "ubuntu": {
          "apt": {
            "packages": ["elasticsearch", "logstash", "kibana"],
            "repository": "deb https://artifacts.elastic.co/packages/8.x/apt stable main",
            "key": "https://artifacts.elastic.co/GPG-KEY-elasticsearch"
          }
        },
        "alpine": {
          "apk": {
            "packages": ["elasticsearch", "logstash", "kibana"]
          }
        }
      },
      "versions": {
        "latest": "8.11",
        "stable": "8.11",
        "lts": "7.17"
      },
      "default_ports": {
        "elasticsearch": 9200,
        "logstash": 5044,
        "kibana": 5601
      }
    },
    "jaeger": {
      "name": "Jaeger",
      "description": "Distributed tracing",
      "install_methods": {
        "universal": {
          "docker": {
            "image": "jaegertracing/all-in-one:latest",
            "ports": ["16686:16686", "14268:14268"]
          }
        }
      },
      "versions": {
        "latest": "1.53",
        "stable": "1.53",
        "beta": "1.54"
      },
      "default_port": 16686
    }
  },
  "cli_tools": {
    "cursor": {
